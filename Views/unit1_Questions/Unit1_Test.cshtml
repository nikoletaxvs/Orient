@model IEnumerable<unit1_question>
@{
    ViewData["Title"] = "Unit1_Test";
}

<h1>Unit1_Test</h1>

<form>
@foreach(var obj in Model)
{
    <div class="container">
        <h6>@obj.title</h6>
        <h5>@obj.description</h5>
        <div class="row p-1">
            <div class="col mx-1 btn btn-primary" value="correct" asp-action="Check">@obj.correctAnswear</div>
            <div class="col mx-1 btn btn-primary" value="incorrect" asp-action="Check">@obj.answear1</div>
        </div>
        <div class="row p-1">
            <div class="col mx-1 btn btn-primary" value="incorrect" asp-action="Check">@obj.answear2</div>
            <div class="col mx-1 btn btn-primary" value="incorrect" asp-action="Check">@obj.answear3</div>
        </div>
    </div>
}
</form>
<div>
    @TempData["buttonVal"]
</div>